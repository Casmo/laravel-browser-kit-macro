{
    "name": "pbmedia/laravel-browser-kit-macro",
    "description": "A macro to use the Laravel 5.3 testing layer inside your Laravel 5.4+ tests",
    "keywords": [
        "pascalbaljetmedia",
        "laravel-browser-kit"
    ],
    "homepage": "https://github.com/pascalbaljetmedia/laravel-browser-kit-macro",
    "type": "library",
    "require": {
        "laravel/browser-kit-testing": "^5.0",
        "illuminate/http": "^5.8",
        "illuminate/support": "^5.8"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "~7.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@pascalbaljetmedia.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pbmedia\\LaravelBrowserKitMacro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pbmedia\\LaravelBrowserKitMacro\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pbmedia\\LaravelBrowserKitMacro\\BrowserKitMacroServiceProvider"
            ]
        }
    }
}