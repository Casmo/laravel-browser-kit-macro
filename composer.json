{
    "name": "pbmedia/laravel-browser-kit-macro",
    "description": "A macro to use the Laravel 5.3 testing layer inside your Laravel 5.4+ tests",
    "type": "library",
    "require": {
        "laravel/browser-kit-testing": "^2.0",
        "illuminate/http": "5.4.* || 5.5.* || 5.6.*",
        "illuminate/support": "5.4.* || 5.5.* || 5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0|~6.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pascal Baljet",
            "email": "pascal@pascalbaljetmedia.com"
        }
    ],
     "autoload": {
        "psr-4": {
            "Pbmedia\\LaravelBrowserKitMacro\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pbmedia\\LaravelBrowserKitMacro\\BrowserKitMacroServiceProvider"
            ]
        }
    }
}